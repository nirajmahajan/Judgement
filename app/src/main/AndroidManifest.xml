<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="android.example.judgement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Game.template.game_edit_player"></activity>
        <activity
            android:name=".Game.template.GameTemplate"
            android:label="@string/title_activity_game_template"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity android:name=".Initialise.About" />
        <activity android:name=".Initialise.Init_Dealer" />
        <activity android:name=".Initialise.TemplateActivity" />
        <activity android:name=".Initialise.Resume_Page" />
        <activity
            android:name=".Initialise.Init_Players"
            android:label="@string/title_activity_init__players"
            android:parentActivityName=".Initialise.MainActivity"
            tools:targetApi="jelly_bean">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.judgement.Initialise.MainActivity" />
        </activity>
        <activity android:name=".Initialise.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>